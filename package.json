{
  "name": "workspace-matching-platform",
  "version": "1.0.0",
  "private": true,
  "packageManager": "yarn@4.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "yarn workspace next dev",
    "build": "yarn workspace next build",
    "start": "yarn workspace next start",
    "lint": "yarn workspace next lint",
    "format": "yarn workspace next format",
    "type-check": "yarn workspace next type-check",
    "db:start": "docker-compose up -d",
    "db:stop": "docker-compose down",
    "db:reset": "yarn workspace supabase db:reset"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7"
  }
}
